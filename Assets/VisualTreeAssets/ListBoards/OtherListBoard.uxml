    <UI.Control name="frame-viewport" picking-mode="IgnoreSelf" style="align-items: center; justify-content: center; background-color: rgba(230, 230, 230, 0); position: absolute; left: 0; top: 0; right: 0; bottom: 0;">
        <UI.Control name="frame-container" picking-mode="IgnoreSelf" style="flex-direction: row; width: 1280px;">
            <UI.Control name="spacer-left" picking-mode="IgnoreSelf" style="flex-grow: 1;" />
            <Controls.Raw.DiamondFrameVertical color="#333333FF" corner-radius="4" name="frame" unfolded="true">
                <UI.Control name="padding-box" style="flex-grow: 1; width: 480px; height: 640px; padding-top: 4px; padding-right: 4px; padding-bottom: 4px; padding-left: 4px;">
